{
  "name": "obsidian-wakatime",
  "version": "1.4.0",
  "description": "Automatic time tracking and metrics generated from your Obsidian usage activity.",
  "main": "main.js",
  "packageManager": "pnpm@10.28.1",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && esbuild src/main.ts --bundle --platform=node --outdir=dist --tree-shaking=true --minify --external:obsidian",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "check": "biome check",
    "fix": "biome check --write --unsafe"
  },
  "keywords": [
    "time tracker",
    "time tracking"
  ],
  "author": "",
  "license": "BSD",
  "dependencies": {
    "adm-zip": "0.5.16",
    "obsidian": "^1.8.7",
    "which": "5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/adm-zip": "0.5.7",
    "@types/node": "16.18.126",
    "@types/which": "3.0.4",
    "esbuild": "0.27.2",
    "obsidian": "1.8.7",
    "typescript": "5.9.3"
  }
}
